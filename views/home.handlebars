    <div class="container w-auto h-auto rounded">
        <div class="homedisplay">
            <div class="row">

                <div class="col-md-5">
                    <div class="card h-auto w-auto"
                        style="border-style: inset; border-color: black; border-width: 5px; background-color: white; background-size: 100% 100%;">
                        <div class="formcarddisplay my-3 mx-3">
                            <h3 class="text-center" style="font-family: 'Bangers', cursive;">Register</h3>
                            <form>
                                <div class="form-group">
                                    <label for="firstnameinput">First Name:</label>
                                    <input type="text" class="form-control" id="firstnameinput">
                                </div>
                                <div class="form-group">
                                    <label for="usernameinput">Username:</label>
                                    <input type="text" class="form-control" id="usernameinput">
                                </div>
                                <div class="form-group">
                                    <label for="passwordinput">Password:</label>
                                    <input type="password" class="form-control" id="passwordinput">
                                </div>
                                <div class="text-center">
                                    <div class="warning"></div>
                                    <button type="button" class="btn btn-primary" id="registerbtn">Register</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="col-md-2 text-center"
                    style="margin-top: 15%; -webkit-text-stroke-width: 1px; -webkit-text-stroke-color: white;">
                    <h1>or</h1>
                </div>

                <div class="col-md-5 mb-5">
                    <div class="card h-auto w-auto"
                        style="border-style: inset; border-color: black; border-width: 5px; background-color: white; background-size: 100% 100%; margin-top: 10%;">
                        <div class="formcarddisplay my-3 mx-3">
                            <h3 class="text-center" style="font-family: 'Bangers', cursive;">Login</h3>
                            <form>
                                <div class="form-group">
                                    <label for="usernameinput">Username:</label>
                                    <input type="text" class="form-control" id="usernameinputlogin">
                                </div>
                                <div class="form-group">
                                    <label for="passwordinput">Password:</label>
                                    <input type="password" class="form-control" id="passwordinputlogin">
                                </div>
                                <div class="text-center">
                                    <div class="warninglogin"></div>
                                    <button type="button" class="btn btn-primary" id="loginbtn">Login</button>
                                </div>
                            </form>
                        </div>
